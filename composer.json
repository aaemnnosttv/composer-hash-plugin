{
    "name": "aaemnnosttv/composer-hash-plugin",
    "description": "Composer plugin for writing the current composer version hash to a file on install/update.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Mattson",
            "email": "me@aaemnnost.tv"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "composer-plugin-api": "^1.1 || ^2.0",
        "ext-json": "*",
        "php": ">=5.3"
    },
    "require-dev": {
        "composer/composer": "^2"
    },
    "extra": {
        "class": "ComposerHash\\Plugin"
    }
}
